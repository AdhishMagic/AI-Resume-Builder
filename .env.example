# Copy to .env.local
# Vite only exposes env vars prefixed with VITE_

# Default: disabled so the app can run without errors.
# Set to true only after you have a valid (non-leaked) Gemini key.
VITE_ENABLE_GEMINI=false

VITE_GOOGLE_API_KEY=

# --- Backend AI Editor (recommended) ---
# Run the local API server via: `node server/index.mjs`
ENABLE_GEMINI=false
GOOGLE_API_KEY=
AI_EDITOR_PORT=5174
